#include<iostream>
using namespace std;
int main(){
    int i,n1=0,n2=1,n3,n;
    cout<<"Enter the number"<<endl;
    cin>>n;
    if(n==0 || n==1){
        cout<<"0";
    }
    else{
        cout<<n1<<" " <<n2<<" ";
    }
    for(i=2;i<n;i++){
        n3=n1+n2;
        cout<<n3<<" ";
        n1=n2;
        n2=n3;
    }
}
