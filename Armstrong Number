#include<iostream>
using namespace std;
int main(){
    int i,n,r,sum=0,temp;
    cout<<"Enter the number"<<endl;
    cin>>n;
    temp=n;
    while(n>0){
        r=n%10;
        sum=sum+(r*r*r);
        n=n/10;
    }
    if(temp==sum){
        cout<<temp<<" Armstrong Number";
    }
    else{
        cout<<temp<<" Not Armstrong Number";
    }
    return 0;
}
