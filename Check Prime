#include<iostream>
using namespace std;
int main(){
    int i,n,j=0;
    cout<<"Enter the value for checking number is prime or not"<<endl;
    cin>>n;
    for(i=1;i<=n;i++){
        if((n%i)==0){
            j++;
        }
    }
    if(j==2)
        cout<<n<<" is a prime"<<endl;
    else
        cout<<n<<" is not prime"<<endl;
    return 0;
}
