#include<iostream>
using namespace std;
int main(){
    int i,n,sum=0,temp,r;
    cout<<"Enter the no. for checking it is palindrome or not"<<endl;
    cin>>n;
    temp=n;
    while(n>0){
        r=n%10;
        sum=(sum*10)+r;
        n=n/10;
    }
    if(temp==sum){
        cout<<"Number is Palindrome";
    }
    else{
        cout<<"Number is not palindrome";
    }
}
